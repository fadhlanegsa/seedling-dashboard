<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /seedling-dashboard/seedling-dashboard/public/

    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)$ index.php?url=$1 [QSA,L]
</IfModule>

Options -Indexes

<FilesMatch "^\." >
    Order allow,deny
    Deny from all
</FilesMatch>

<FilesMatch "\.(sql|md|log)$">
    Order allow,deny
    Deny from all
</FilesMatch>