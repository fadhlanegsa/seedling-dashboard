<IfModule mod_rewrite.c>
    RewriteEngine On
    # RewriteBase /

    RewriteCond %{REQUEST_URI} !^/public/
    RewriteRule ^(.*)$ public/$1 [L]
</IfModule>

Options -Indexes

<FilesMatch "^\." >
    Order allow,deny
    Deny from all
</FilesMatch>

<FilesMatch "\.(sql|md|log)$">
    Order allow,deny
    Deny from all
</FilesMatch>